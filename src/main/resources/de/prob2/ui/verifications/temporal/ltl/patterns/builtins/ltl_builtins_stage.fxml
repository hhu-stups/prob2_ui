<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<fx:root type="Stage" title="%verifications.temporal.ltl.pattern.builtins.title" minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1">
	<scene>
		<Scene stylesheets="@/prob.css" height="400.0" width="600.0">
			<!-- TableView does not extend Pane, so we have to wrap it in a Pane so we can add the menu bar to it on Mac. -->
			<AnchorPane>
				<TableView fx:id="tvPatterns" AnchorPane.bottomAnchor="0.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
					<columnResizePolicy>
						<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
					</columnResizePolicy>
					<columns>
						<TableColumn fx:id="pattern" editable="false" prefWidth="20.0" text="%verifications.temporal.ltl.pattern.builtins.pattern" />
						<TableColumn fx:id="description" editable="false" prefWidth="200.0" text="%verifications.temporal.ltl.pattern.builtins.description" />
					</columns>
				</TableView>
			</AnchorPane>
		</Scene>
	</scene>
</fx:root>
