<?xml version="1.0" encoding="UTF-8"?>

<?import de.prob2.ui.output.PrologOutput?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<?import org.fxmisc.flowless.VirtualizedScrollPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<fx:root type="Stage" title="%menu.advanced.items.prologoutput.title" minWidth="320" minHeight="240" maxWidth="800.0" maxHeight="600.0" xmlns:fx="http://javafx.com/fxml/1">
	<scene>
		<Scene stylesheets="@/prob.css">
			<StackPane>
				<VBox>
					<VirtualizedScrollPane minWidth="200.0" minHeight="200.0" prefWidth="800.0" prefHeight="600.0">
						<content>
							<PrologOutput/>
						</content>
					</VirtualizedScrollPane>
					<HBox>
						<Button fx:id="clearButton" styleClass="button-dark2" maxWidth="Infinity" HBox.hgrow="ALWAYS" text="%menu.advanced.items.prologoutput.clear"/>
					</HBox>
				</VBox>
			</StackPane>
		</Scene>
	</scene>
</fx:root>
