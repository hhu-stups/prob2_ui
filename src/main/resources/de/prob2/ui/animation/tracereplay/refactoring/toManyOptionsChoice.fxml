<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Dialog?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" type="Dialog" resizable="true">
	<dialogPane>
		<DialogPane>
			<content>
				<SplitPane dividerPositions="0.5" prefHeight="150.0" prefWidth="100.0">
					<AnchorPane fx:id="oldInfosPane" minHeight="50.0" minWidth="50.0" prefHeight="0.0"  prefWidth="${node.parent.width}">
						<VBox fx:id="oldInfos" prefHeight="160.0" prefWidth="100.0"/>
					</AnchorPane>
					<AnchorPane fx:id="newInfosPane" minHeight="50.0" minWidth="50.0" prefHeight="160.0"  prefWidth="${node.parent.width}">
						<VBox fx:id="newInfos" prefHeight="160.0" prefWidth="100.0"/>
					</AnchorPane>
				</SplitPane>
			</content>
		</DialogPane>
	</dialogPane>
</fx:root>
