<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Alert?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.TextArea?>

<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<fx:root type="Alert" resizable="true" alertType="ERROR" xmlns:fx="http://javafx.com/fxml/1">
	<dialogPane>
		<DialogPane>
			<padding>
				<Insets bottom="0" top="0" left="10" right="10"/>
			</padding>
			<content>
				<Text fx:id="error" lineSpacing="5"/>
			</content>
			<expandableContent>
				<TextArea fx:id="taError" editable="false" wrapText="true"/>
			</expandableContent>
		</DialogPane>
	</dialogPane>
</fx:root>
