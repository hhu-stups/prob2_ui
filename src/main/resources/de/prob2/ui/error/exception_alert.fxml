<?xml version="1.0" encoding="UTF-8"?>

<?import de.prob2.ui.error.ErrorTableView?>
<?import javafx.scene.control.Alert?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<fx:root type="Alert" resizable="true" alertType="ERROR" xmlns:fx="http://javafx.com/fxml/1">
	<dialogPane>
		<DialogPane>
			<content>
				<VBox fx:id="contentVBox">
					<Label fx:id="label" VBox.vgrow="NEVER"/>
					<ErrorTableView fx:id="proBErrorTable" VBox.vgrow="ALWAYS"/>
				</VBox>
			</content>
			<expandableContent>
				<TextArea fx:id="stackTraceTextArea" editable="false"/>
			</expandableContent>
		</DialogPane>
	</dialogPane>
</fx:root>
