<fx:root title="%vomanager.title" type="HistoryTable" width="600" height="400" minHeight="600" minWidth="400" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">

	<scene>
		<Scene stylesheets="@/prob.css">
			<VBox>



						<TableView fx:id="table" >
							<columns>

								<TableColumn fx:id="name" minWidth="70.0"
											 text="%vomanager.table.history.column.name" >
									<cellValueFactory><PropertyValueFactory property="id" />
									</cellValueFactory>
								</TableColumn>

								<TableColumn fx:id="expression" minWidth="70.0"
											 text="%vomanager.table.history.column.expression">
									<cellValueFactory><PropertyValueFactory property="expression" />
									</cellValueFactory>
								</TableColumn>

								<TableColumn fx:id="requirement" minWidth="70.0"
											 text="%vomanager.table.history.column.requirement">
									<cellValueFactory><PropertyValueFactory property="requirement" />
									</cellValueFactory>
								</TableColumn>
							</columns>
							<items>
								<FXCollections fx:factory="observableArrayList">

								</FXCollections>
							</items>

						</TableView>

			</VBox>
		</Scene>
	</scene>
</fx:root>
