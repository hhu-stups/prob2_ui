<?xml version="1.0" encoding="UTF-8"?>

<?import de.prob2.ui.vomanager.VOEditingBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root type="VOEditingBox" styleClass="border" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1">
	<HBox styleClass="border">
		<padding>
			<Insets left="5.0"/>
		</padding>
		<Label fx:id="voTitleLabel" text="%vomanager.title.VO" styleClass="title" maxWidth="Infinity"/>
	</HBox>
	<VBox>
		<Label text="%vomanager.edit.vo.name"/>
		<TextField fx:id="tfVOName" maxWidth="200.0" style="-fx-control-inner-background: #f8f8f8; -fx-border-color: -prob-aqua; -fx-border-width: 2;"/>
	</VBox>
	<VBox>
		<Label text="%vomanager.edit.vo.linkRequirement"/>
		<ChoiceBox fx:id="cbLinkRequirementChoice"/>
	</VBox>
	<VBox>
		<Label text="%vomanager.edit.vo.linkMachine"/>
		<ChoiceBox fx:id="cbVOLinkMachineChoice"/>
	</VBox>
	<VBox>
		<Label text="%vomanager.edit.vo.expression"/>
		<ComboBox fx:id="cbVOExpression" editable="true"/>
	</VBox>
	<Button fx:id="applyVOButton" styleClass="button-blue" text="%common.buttons.apply" onAction="#applyVO"/>
</fx:root>
